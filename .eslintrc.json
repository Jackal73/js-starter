{
  "env": {
    "browser": true,
    "es6": true,
    "node": false
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "array-callback-return": "warn",
    "no-confusing-arrow": "off",
    "no-unused-vars": "warn"
  }
}
